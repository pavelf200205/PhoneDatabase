<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ua.pavelf200205.phonedatabase.PhoneOverviewController">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="524.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="phoneTable">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="108.0" text="ID" />
          <TableColumn fx:id="lastNameColumn" prefWidth="115.0" text="Прізвище" />
          <TableColumn fx:id="firstNameColumn" prefWidth="110.0" text="Ім'я" />
            <TableColumn fx:id="patronymicColumn" prefWidth="126.0" text="По батькові" />
            <TableColumn fx:id="accountIDColumn" prefWidth="121.0" text="Номер рахунку" />
        </columns>
      </TableView>
      <ButtonBar prefHeight="37.0" prefWidth="300.0">
        <buttons>
             <Button text="Додати" />
            <Button mnemonicParsing="false" text="Видалити" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="112.0" text="Фільтрувати" />
        </buttons>
      </ButtonBar>
   </children>
</FlowPane>
